
@{
    ViewBag.Title = "Etiquetas Geradas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form id="form-report" action="" method="GET" target="_blank">
    <div class="card mb-2 mt-4">
        <div class="card-header">
            <span class="h2">Etiquetas Geradas</span>
        </div>
        <div class="card-body">
            <div class="form-group">
                <div class="col-md-4">
                    <label class="col-form-label">Data Inicio</label>
                </div>
                <div class="col-md-12">
                    <input type="date" id="dataInicio" name="dataInicio" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-4">
                    <label class="col-form-label">Data Fim</label>
                </div>
                <div class="col-md-12">
                    <input type="date" id="dataFim" name="dataFim" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-4">
                    <label class="col-form-label">Exportar Para</label>
                </div>
                <div class="col-md-12">
                    <select class="form-control" id="exportarPara">
                        <option value="excel">Excel</option>
                    </select>
                </div>
            </div>
        </div>
    </div>
</form>

<div class="row">
    <div class="col-md-12">
        <button type="button" class="btn btn-primary mt-2 mb-3" id="btnGerarRelatorio">Gerar relatório</button>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/EtiquetasGeradas")
}


